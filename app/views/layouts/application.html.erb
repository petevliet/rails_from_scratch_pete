<!DOCTYPE html>
<html>
<head>
  <title>CrudFromScratchPete</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <% if flash[:notice] %>
    <center><%= notice %></center>
<% elsif flash[:alert] %>
    <center><%= alert %></center>
<% end %>

<%= yield %>

</body>
</html>
